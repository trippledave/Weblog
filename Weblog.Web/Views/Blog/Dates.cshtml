@using Weblog.Web.Models.Weblog
@model List<DateTime>

@if (Model.Count > 0)
{
    <ol class="list-unstyled">
        @foreach (DateTime item in Model)
        {
            <li>
                @Ajax.ActionLink(item.ToString("MMMM yyyy"), "DisplayEntriesByDate", new { month = item.Month, year = item.Year }, new AjaxOptions
           {
               UpdateTargetId = "displayEntries",
               InsertionMode = InsertionMode.Replace,
               HttpMethod = "GET",
               OnSuccess = "SiteHelpers.newPagination()"
           }, new { })

            </li>
        }
    </ol>
}
