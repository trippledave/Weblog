@using Weblog.Web.Models.Weblog
@model List<DateModel>

@if (Model.Count > 0)
{
    <ol class="list-unstyled">
        @foreach (DateModel item in Model)
        {
            <li>
                @Ajax.ActionLink(item.DateString, "DisplayEntriesByDate", new { month = item.Month, year = item.Year }, new AjaxOptions
           {
               UpdateTargetId = "displayEntries",
               InsertionMode = InsertionMode.Replace,
               HttpMethod = "GET"
           }, new { })

            </li>
        }
    </ol>
}
